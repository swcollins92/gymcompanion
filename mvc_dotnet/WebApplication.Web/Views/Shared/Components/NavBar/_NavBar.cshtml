@model User
@{
    ViewData["Title"] = "_Navbar";
}



@if (Model == null)
{

    <li><a asp-area="" asp-controller="Account" asp-action="Login">Login</a></li>
}
else if (Model.Role == "Admin")
{
    <li><a asp-area="" asp-controller="Account" asp-action="Index">Welcome Admin: @Model.Username</a></li>
    <li><a asp-area="" asp-controller="MemberInfo" asp-action="ViewSchedules">View Gym Schedules</a></li>
    <li><a asp-area="" asp-controller="Account" asp-action="Logoff">Logout</a></li>

}
else
{
    <li><a asp-area="" asp-controller="Account" asp-action="Index">Welcome @Model.Username</a></li>
    <li><a asp-area="" asp-controller="MemberInfo" asp-action="ViewSchedules">View Gym Schedules</a></li>
    <li><a asp-area="" asp-controller="Account" asp-action="Logoff">Logout</a></li>
    @if (Model.Role == "Member")
    {
        <li><a asp-area="" asp-controller="MemberInfo" asp-action="MemberTimelog">Check In/Check Out</a></li>
    }
    else if(Model.Role == "Employee")
    {
        <li><a asp-area="" asp-controller="MemberInfo" asp-action="EmployeeTimelog">Check In/Check Out Gym member</a></li>
    }
    
}