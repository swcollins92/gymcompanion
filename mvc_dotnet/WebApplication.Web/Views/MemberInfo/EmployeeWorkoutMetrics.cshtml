@model WebApplication.Web.Models.MemberInfo.Timelog

@{
    ViewData["Title"] = "EmployeeWorkoutMetrics";
}

<h2>EmployeeWorkoutMetrics</h2>

<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            <form asp-controller="MemberInfo" asp-action="EmployeeWorkoutById" method="get">

                <select asp-for="MemberId" asp-items="Model.AllMembers" class="form-control">
                    <option value="">Select a Gym Member</option>
                </select><br />
                <div class="form-group">
                    <input type="submit" value="Submit" class="btn btn-default" />
                </div>
            </form>
        </div>
    </div>
</div>
