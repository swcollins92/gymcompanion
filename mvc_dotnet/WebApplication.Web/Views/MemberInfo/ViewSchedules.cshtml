@model WebApplication.Web.Models.GymDetails.ViewScheduleModel

@{
    ViewData["Title"] = "ViewSchedules";
}

<h2>ViewSchedules</h2>

<p>
    <a asp-controller="MemberInfo" asp-action="EditSchedule">Create New</a>
</p>
<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var schedule in Model.AllSchedules)
        {
            <tr>


                <td>@schedule.Name</td>

                <td>@schedule.Description</td>
                <td>@schedule.Date</td>

            </tr>
        }
    </tbody>
</table>
